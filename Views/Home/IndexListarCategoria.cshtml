<div class="jumbotron">
    <h1>Master Chef</h1>
</div>

@model IEnumerable<MasterChef.Entity.Categoria>

@{
    ViewBag.Title = "IndexListarCategoria";
}



<p>
    @Html.ActionLink("Inserir Categoria","IndexCadastroCategoria")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TITULO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESCRICAO)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TITULO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DESCRICAO)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CATEGORIAID }) |
            @Html.ActionLink("Details", "Details", new { id=item.CATEGORIAID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CATEGORIAID })
        </td>
    </tr>
}

</table>
